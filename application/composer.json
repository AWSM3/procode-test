    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry",
            "email": "welldone1204@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-pdo":"*",
        "ext-json":"*",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "vlucas/phpdotenv": "^2.5",
        "slim/twig-view": "^2.4",
        "robmorgan/phinx": "^0.10.6",
        "ramsey/uuid": "^3.8",
        "gufy/pdftohtml-php": "^2.0"
    },
    "require-dev": {
        "larapack/dd": "^1.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
